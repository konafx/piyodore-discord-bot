  "packageManager": "yarn@3.3.1",
  "scripts": {
    "start": "yarn run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts"
  },
  "dependencies": {
    "discord.js": "^14.7.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}

{
  "name": "piyodore-discord-bot",
  "packageManager": "pnpm@7.24.3",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "schema:format": "prisma format",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:reset": "prisma migrate reset",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node ./.dist/main.ts",
    "prebuild": "rimraf ./.dist",
    "build": "pnpm db:generate && tsc -p ./",
    "dev": "pnpm db:generate && ts-node src/main.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ts-node --ignore '*.test.ts' src/main.ts",
    "dev:container": "docker compose up -d",
    "dev:container:nocache": "pnpm run dev:container --build"
  },
  "dependencies": {
    "@prisma/client": "4.8.1",
    "discord-api-types": "^0.37.26",
    "discord.js": "^14.7.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "prisma": "4.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}

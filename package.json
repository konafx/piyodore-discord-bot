{
  "name": "piyodore-discord-bot",
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node ./.dist/main.ts",
    "prebuild": "rimraf ./.dist",
    "build": "yarn run prebuild && tsc -p ./",
    "dev": "ts-node src/main.ts",
    "dev:watch": "nodemon --watch 'src/**/*.ts' --exec yarn run dev"
  },
  "dependencies": {
    "discord-api-types": "^0.37.26",
    "discord.js": "^14.7.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
